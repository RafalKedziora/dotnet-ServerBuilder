{
  "PaperApi": {
    "BaseUrl": "https://papermc.io/api/",
    "GetVersions": "v2/projects/paper",
    "GetBuildsData": "v2/projects/paper/versions/{selectedServerVersion}/builds",
    "EngineFileName": "paper-{selectedServerVersion}-{buildNumber}.jar",
    "GetServerInstance": "v2/projects/paper/versions/{selectedServerVersion}/builds/{buildNumber}/downloads/paper-{selectedServerVersion}-{buildNumber}.jar"
  },
  "PurpurApi": {
    "BaseUrl": "https://api.purpurmc.org/",
    "GetVersions": "v2/purpur",
    "GetBuildsData": "v2/purpur/{selectedServerVersion}/latest",
    "GetServerInstance": "v2/purpur/{selectedServerVersion}/latest/download",
    "EngineFileName": "purpur-{selectedServerVersion}-{buildNumber}.jar"
  },
  "PufferfishApi": {
    "BaseUrl": "https://ci.pufferfish.host/",
    "GetVersions": "api/json",
    "GetBuildsData": "job/{selectedServerVersion}/api/json",
    "GetBuildStatusInfo": "job/{selectedServerVersion}/{selectedBuildNumber}/api/json",
    "GetServerInstance": "job/{selectedServerVersionGroup}/{selectedBuildNumber}/artifact/{downloadEnginePath}"
  },
  "FabricApi": {
    "BaseUrl": "https://meta.fabricmc.net/",
    "GetVersions": "v2/versions/game",
    "GetLoaderData": "v2/versions/loader",
    "GetInstallerData": "v2/versions/installer",
    "GetServerInstance": "v2/versions/loader/{selectedServerVersion}/{selectedLoaderVersion}/{selectedInstallerVersion}/server/jar"
  }
}